{
  "version": "0.0.0",
  "name": ""踪迹 At Your Feet",
  "manifest_version": 2,
  "description": "踪迹 At Your Feet",
  "browser_action": {
    "default_title": "踪迹 At Your Feet",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["content/content.css"],
      "js": ["content/content.js"]
    }
  ],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "fileBrowserHandler",
    "history",
    "idle",
    "management",
    "notifications",
    "tabs",
    "geolocation",
    "storage",
    "https://*/*"
  ],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:; font-src https://at.alicdn.com/;"
}
